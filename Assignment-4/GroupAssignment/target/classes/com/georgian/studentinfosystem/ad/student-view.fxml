<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href=""?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="560.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.georgian.studentinfosystem.ad.StudentController">

    <children>
        <Label layoutX="226.0" layoutY="26.0" prefHeight="59.0" prefWidth="148.0" text="Student" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font size="41.0" />
            </font>
        </Label>


        <Label layoutX="110.0" layoutY="115.0" prefHeight="30.0" prefWidth="114.0" text="StudentID:" textFill="#b9b9b9">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="StudentID" layoutX="211.0" layoutY="115.0" prefHeight="30.0" prefWidth="186.0" />

        <Label layoutX="110.0" layoutY="159.0" prefHeight="30.0" prefWidth="125.0" text="Name:" textFill="#b9b9b9">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="Name" layoutX="211.0" layoutY="158.0" prefHeight="30.0" prefWidth="186.0" />

        <Label layoutX="110.0" layoutY="200.0" prefHeight="30.0" prefWidth="136.0" text="Age:" textFill="#b9b9b9">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="Age" layoutX="211.0" layoutY="200.0" prefHeight="30.0" prefWidth="186.0" />

        <Label layoutX="110.0" layoutY="242.0" prefHeight="30.0" prefWidth="136.0" text="Address:" textFill="#b9b9b9">
